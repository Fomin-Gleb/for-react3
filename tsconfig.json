{
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "types": ["react/next", "react-dom/next"],
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"]
  },
  "exclude": ["node_modules"],
  "include": ["src"],
  "allowJs": true,
}
